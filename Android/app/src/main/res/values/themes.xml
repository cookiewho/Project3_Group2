<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="RocketCornerTheme" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->

        <item name="colorPrimary">@color/red</item>
        <item name="colorPrimaryDark">@color/red_dark</item>
        <item name="colorAccent">@color/red_light</item>
        <item name="android:textColor">@color/off_grey</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="RocketCornerThemeGood" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue</item>
        <item name="colorPrimaryDark">@color/blue_dark</item>
        <item name="colorAccent">@color/blue_light</item>
        <!-- Customize your theme here. -->
    </style>


    <style name="myEditTextStyle">
        <item name="android:colorControlNormal">#ffff</item>
        <item name="android:colorControlActivated">#ffff</item>
        <item name="android:colorControlHighlight">#ffff</item>
        <item name="android:colorAccent">#ffff</item>
        <item name="android:textColorHint">#ffff</item>
    </style>

    <style name="RocketCornerTheme.NoNav">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.RocketCorner.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.RocketCorner.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>