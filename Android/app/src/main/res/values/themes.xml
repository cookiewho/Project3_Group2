<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.RocketCorner" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/teal_200</item>
        <item name="colorPrimaryDark">#009688</item>
        <item name="colorAccent">@color/teal_200</item>
        <!-- Customize your theme here. -->
    </style>


    <style name="myEditTextStyle">
        <item name="android:colorControlNormal">#ffff</item>
        <item name="android:colorControlActivated">#ffff</item>
        <item name="android:colorControlHighlight">#ffff</item>
        <item name="android:colorAccent">#ffff</item>
        <item name="android:textColorHint">#ffff</item>
    </style>

    <style name="Theme.RocketCorner.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.RocketCorner.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.RocketCorner.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>